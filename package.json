{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Build with Webpack4",
  "main": "index.js",
  "scripts": {
    "start": "node cli",
    "prod": "yarn build --mode production",
    "prod:server": "yarn watch --mode production",
    "dev": "yarn build --mode development",
    "dev:server": "yarn watch --mode development",
    "dev:debug": "yarn watch:debug --mode development",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --config 'webpack/webpack.js'",
    "watch": "yarn build --watch",
    "watch:debug": "yarn debug --watch",
    "build": "webpack --config 'webpack/webpack.js'"
  },
  "author": "Pepe Miyashiro",
  "license": "ISC",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^1.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.24.1",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-nested": "^3.0.0",
    "postcss-remove-root": "^0.0.2",
    "postcss-reporter": "^5.0.0",
    "postcss-responsive-type": "^1.0.0",
    "sanitize.css": "^5.0.0",
    "webcomponents-lite": "^0.6.0",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  }
}
