{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Build with Webpack4",
  "main": "index.js",
  "scripts": {
    "build:prod": "yarn watch --mode production",
    "dev": "yarn watch --mode development",
    "dev:debug": "yarn watch:debug --mode development",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "build": "node ./node_modules/webpack/bin/webpack.js",
    "watch": "yarn build --watch",
    "watch:debug": "yarn debug --watch"
  },
  "author": "Pepe Miyashiro",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "^2.1.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  }
}
